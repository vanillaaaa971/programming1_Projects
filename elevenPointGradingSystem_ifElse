#include <stdio.h>

    int main () {
    int studentGrade;
    printf("Enter your Grade: ");
    scanf("%d", &studentGrade);

    if (studentGrade >= 99 && studentGrade <= 100) {
        printf("Your grade is 1.00\n");
    } else if (studentGrade >= 96 && studentGrade <= 98 ) {
        printf("Your grade is 1.25\n"); 
    } else if (studentGrade >= 93 && studentGrade <= 95 ) {
        printf("Your grade is 1.50\n"); 
    } else if (studentGrade >= 90 && studentGrade <= 93 ) {
        printf("Your grade is 1.75\n");
    }else if (studentGrade >= 87 && studentGrade <= 89 ) {
        printf("Your grade is 2.00\n"); 
    } else if (studentGrade >= 84 && studentGrade <= 86 ) {
        printf("Your grade is 2.25\n");
    } else if (studentGrade >= 81 && studentGrade <= 83 ) {
        printf("Your grade is 2.50\n");
    } else if (studentGrade >= 78 && studentGrade <= 80 ) {
        printf("Your grade is 2.75\n");
    } else if (studentGrade >= 75 && studentGrade <= 77 ) {
        printf("Your grade is 3.00\n");
    } else if (studentGrade >= 72 && studentGrade <= 74 ) {
        printf("Your grade is 4.00\n");
    } else if (studentGrade <= 71) {         
        printf("Your grade is 5.00\n");
    } else printf("Invalid Grade\n");

    
return 0; 
}
